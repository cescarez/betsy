<h2 class="small-glow">Shop Stellar by Seller</h2>
<section class="container">
  <% if @users.nil? %>
    <h2> Currently, there are no active sellers!</h2>
  <% else %>
    <% @users.each do |user| %>
      <li>
        <span><%= link_to user.username, user_path(user.id), class: "hover-5"%></span>
      </li>
      <% end %>
  <% end %>
</section>
