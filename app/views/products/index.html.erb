<main>
  <h2>
  <section class=”product-index”>
    Products
  </section>
  </h2>
  <ul> <% @products.each do |product|  %>
        <li> <%= link_to product.name, product_path(product) %> |
          <%= link_to "Edit", edit_product_path(product[:id]), method: :get%> |
          <%= link_to "Delete", product_path(product[:id]), method: :delete, data: {confirm: "Are you sure?"} %> </li></li>
    <% end %>
  </ul>
</main>